{
  "auth": {
    "web": {
      "authCookie": {
        "name": "SID",
        "expiresAfterDays": 365,
        "path": "/",
        "secure": false
      }
    }
  },
  "id": {
    "web": {
      "forwardedForIpHeaderName": "X-Forwarded-For"
    }
  },
  "dataservices": {
    "profiles": {
      "forbiddenUsernames": [
        "api",
        "show",
        "login",
        "signup",
        "signin",
        "settings",
        "explore"
      ]
    }
  },
  "services": {
    "sessions": {
      "sessionsRedisPrefix": "ses",
      "profilesToSessionsRedisPrefix": "pses"
    }
  },
  "handlers": {
    "spanner": {
      "poolOptions": {}
    },
    "redis": {
      "url": "redis://doojon_redis:6379"
    }
  }
}
